<h1> Create a VendorSweet!<h1>

<% if flash[:some_errors] %>
    <% flash[:some_errors].each do |message| %>
        <%= message %>
    <% end %>
<% end %>

<%= form_for @vendorsweet do |f| %>

<%= f.label :vendor_id %>
<%= f.collection_select :vendor_id, @vendors, :id, :name %>

<%= f.label :sweet_id %>
<%= f.collection_select :sweet_id, @sweets, :id, :name %>

<%= f.label :price %>
<%= f.number_field :price %>

<%= f.submit %>

<% end %>